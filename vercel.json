{
  "version": 2,
  "builds": [
    {
      "src": "apps/portifolio/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "apps/electoral-system-client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "apps/joystick-client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "apps/lojinha-simples/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "apps/rgbwallet/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "apps/secret-santa-client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "apps/video-project-manage/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/electoral-system/(.*)",
      "dest": "/apps/electoral-system-client/dist/$1"
    },
    {
      "src": "/joystick/(.*)",
      "dest": "/apps/joystick-client/build/$1"
    },
    {
      "src": "/lojinha-simples/(.*)",
      "dest": "/apps/lojinha-simples/dist/$1"
    },
    {
      "src": "/rgbwallet/(.*)",
      "dest": "/apps/rgbwallet/build/$1"
    },
    {
      "src": "/secret-santa/(.*)",
      "dest": "/apps/secret-santa-client/dist/$1"
    },
    {
      "src": "/video-project-manage/(.*)",
      "dest": "/apps/video-project-manage/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/portifolio/.next/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
